<?xml version="1.0"?>
<configuration>

  <location path="Manage.aspx">
    <system.web>
      <authentication mode="Forms">
        <forms loginUrl="Login.aspx" name="adAuthCookie" timeout="60" path="/"/>
      </authentication>
      <authorization>
        <deny users="?"/>
        <allow users="*"/>
      </authorization>
    </system.web>
  </location>

</configuration>
